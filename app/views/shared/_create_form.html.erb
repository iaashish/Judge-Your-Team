<%= form_for(@team) do |f| %>

<div class="form-group">
<label for="teamnameqq">Team</label>
<%= f.text_field :teamname, class:'form-control', id: 'teamnameqq'%>
</div>

<div class="form-group">
<label for="selectjudge">Select judge</label>
<%= collection_select(:team, :user_id,User.all,:id,:name, {:prompt=>"select a judge"}, {:class=>'form-control', :id=>'selectjudge'})%>
</div>
  <%= f.submit "Create", class: "btn btn-primary", style:"width: 20%;" %>
<% end %>



